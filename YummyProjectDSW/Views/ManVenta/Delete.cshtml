@model yummyApp.Models.Venta

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar venta</h1>

<h3>¿Estás seguro que deseas eliminar el registro?</h3>
<div>
    <h4>Venta</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            ID Usuario
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.idUsuario)
        </dd>
        <dt class = "col-sm-2">
            Fecha de venta
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.fechaVenta)
        </dd>
        <dt class = "col-sm-2">
            Estado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.estado)
        </dd>
        <dt class = "col-sm-2">
            Usuario
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.usuario)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="idVenta" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Regresart</a>
    </form>
</div>
